;; Area of Triangle without function
(let ((base 5)
      (height 8))
  (format t "Area of Triangle: ~a~%" (/ (* base height) 2)))

;; Area of Rectangle without function
(let ((length 4)
      (width 6))
  (format t "Area of Rectangle: ~a~%" (* length width)))

;; Cube of a number without function
(let ((num 3))
  (format t "Cube of 3: ~a~%" (expt num 3)))

;; Fahrenheit to Celsius without function
(let ((fahrenheit 68))
  (format t "Fahrenheit to Celsius (F=68): ~a~%" (/ (* (- fahrenheit 32) 5) 9)))

